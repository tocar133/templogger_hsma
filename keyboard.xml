<?xml version="1.0" encoding="UTF-8"?>
<keyboard>

<layout id="default keyboard">

  <row>

    <key width="2200">
      <default display="⇆" action="tab" />
    </key>
    <key fill='false'>
      <default display="q" />
      <shifted display="Q" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="w" />
      <shifted display="W" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="e" />
      <shifted display="E" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="r" />
      <shifted display="R" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="t" />
      <shifted display="T" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="z" />
      <shifted display="Z" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="u" />
      <shifted display="U" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="i" />
      <shifted display="I" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="o" />
      <shifted display="O" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="p" />
      <shifted display="P" />
    </key>
    <key width="2280">
      <default display="⇦" action="backspace" />
    </key>

    <!--<space fill="true"/>-->

    <key fill='false' width="1100">
      <default display="7" />
      <shifted display="(" />
    </key>

    <key fill='false' width="1100">
      <default display="8" />
      <shifted display=")" />
    </key>

    <key fill='false' width="1100">
      <default display="9" />
      <shifted display="-" />
    </key>
  </row>

  <row>
    <key width="2200">
      <default display="⇧" action="modifier:shift" />
    </key>
    
    <key obey-caps='true' fill='false' width="1100">
      <default display="a" />
      <shifted display="A" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="s" />
      <shifted display="S" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="d" />
      <shifted display="D" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="f" />
      <shifted display="F" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="g" />
      <shifted display="G" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="h" />
      <shifted display="H" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="j" />
      <shifted display="J" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="k" />
      <shifted display="K" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="l" />
      <shifted display="L" />
    </key>

    <key fill='false' width="1100">
      <default display="↑" action="up" />
    </key>
    
    <key width="2280">
      <default display="⏎" action="return"/>
    </key>

    <!--<space fill="true"/>-->

    <key fill='false' width="1100">
      <default display="4" />
      <shifted display="/" />
    </key>
    <key fill='false' width="1100">
      <default display="5" />
      <shifted display="\" />
    </key>
    <key fill='false' width="1100">
      <default display="6" />
      <shifted display="_" />
    </key>

  </row>

  <row>
    <key width="1650">
      <default display="ˢᵗʳᵍ" action="modifier:ctrl" />
    </key>
    <key width="1650">
      <!--<default display="ˢᵖᵃᶜᵉ" action="space" />-->
      <default display="ₗₑₑᵣ" action="space" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="y" />
      <shifted display="Y" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="x" />
      <shifted display="X" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="c" />
      <shifted display="C" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="v" />
      <shifted display="V" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="b" />
      <shifted display="B" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="n" />
      <shifted display="N" />
    </key>
    <key obey-caps='true' fill='false' width="1100">
      <default display="m" />
      <shifted display="M" />
    </key>

    <key fill="false" width="1100">
      <default display="←" action="left" />
    </key>
    <key fill='false' width="1100">
      <default display="↓" action="down" />
    </key>
    <key fill='false' width="1100">
      <default display="→" action="right" />
    </key>
    
    <!--<space fill="true"/>-->
    
    <key fill='false' width="1100">
      <default display="0" />
      <shifted display="." />
    </key>

    <key fill='false' width="1100">
      <default display="1" />
      <shifted display="," />
    </key>

    <key fill='false' width="1100">
      <default display="2" />
      <shifted display=":" />
    </key>

    <key fill='false' width="1100">
      <default display="3" />
      <shifted display=";" />
    </key>
  </row>
  
</layout>

</keyboard>
